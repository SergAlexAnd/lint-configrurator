{
  "name": "lint-configs",
  "version": "1.0.0",
  "description": "Package for automatically adding eslint config and stylelint configs to your project",
  "files": [
    "dist"
  ],
  "bin": {
    "lint-config": "dist/bin/index.js"
  },
  "bugs": "https://github.com/SergAlexAnd/lint-configrurator/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SergAlexAnd/lint-configrurator.git"
  },
  "scripts": {
    "test": "jest --coverage",
    "build": "NODE_ENV=production babel src --out-dir dist --source-maps inline",
    "prepublishOnly": "npm run build",
    "lint": "eslint ."
  },
  "author": "Aleksei Sergienko",
  "license": "ISC",
  "dependencies": {
    "commander": "^9.3.0",
    "inquirer": "^8.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.18.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^28.1.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2"
  }
}
